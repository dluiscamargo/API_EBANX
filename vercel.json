{
  "version": 2,
  "functions": {
    "api/index.php": {
      "runtime": "vercel-php@0.7.0"
    }
  },
  "routes": [
    {
      "src": "/reset",
      "dest": "/api/index.php"
    },
    {
      "src": "/balance(.*)",
      "dest": "/api/index.php"
    },
    {
      "src": "/event",
      "dest": "/api/index.php"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.php"
    }
  ]
} 